{
    "name": "starchphp/starch",
    "description": "A collection of other libraries to build a PSR compatible micro-framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bram Van der Sype",
            "email": "bram.vandersype@gmail.com",
            "homepage": "https://github.com/Brammm"
        }
    ],
    "require": {
        "http-interop/http-middleware": "^0.4",
        "nikic/fast-route": "^1.2",
        "php-di/php-di": "^6.0",
        "zendframework/zend-diactoros": "^1.4"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "^6.1"
    },
    "minimum-stability": "alpha",
    "autoload": {
        "psr-4": {
            "Starch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Starch\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
